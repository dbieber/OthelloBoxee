        x += d[0]
        y += d[1]
        if x==0 or x>8 or y==0 or y>8:
            break
        id = 100+10*x+y
        t = mc.GetActiveWindow().GetLabel(id).GetLabel()
        if t == "C":
            cont = False
        if t == turn:
            tu = True
            cont = False
    if tu:
        while x != x0 or y != y0:    
            x -= d[0]
            y -= d[1]
            id = 100+10*x+y
            mc.GetActiveWindow().GetImage(id-100).SetTexture(img)
            mc.GetActiveWindow().GetLabel(id).SetLabel(turn)
            
]]></onclick>
         </control>


