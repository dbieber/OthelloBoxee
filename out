mc.GetActiveWindow().GetImage(25).SetTexture(img)

dirs = [[1,0],[1,1],[1,-1],[0,-1],[0,1],[-1,-1],[-1,1],[-1,0]]
x0 = 2
y0 = 5
for d in dirs:
    x = x0
    y = y0
    cont = True
    tu = False
    while cont:
        x += d[0]
        y += d[1]
        if x==0 or x>8 or y==0 or y>8:
            break
        id = 100+10*x+y
        t = mc.GetActiveWindow().GetLabel(id).GetLabel()
        if t == "C":
            cont = False
        if t == turn:
            tu = True
            cont = False
    if tu:
        while x != x0 or y != y0:    
            x -= d[0]
            y -= d[1]
            id = 100+10*x+y
            mc.GetActiveWindow().GetImage(id-100).SetTexture(img)
            mc.GetActiveWindow().GetLabel(id).SetLabel(turn)
            
]]></onclick>
         </control>


